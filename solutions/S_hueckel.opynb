      "source": [
        "# Orthonormal basis\n",
        "print(\"Orthonormal basis\")\n",
        "print(np.dot(c_eth.transpose(), c_eth))\n",
        "\n",
        "# Diagonalizing M\n",
        "print(\"\")\n",
        "print(\"Diagonalize M\")\n",
        "print(np.dot(c_eth.transpose(), np.dot(M_eth, c_eth)))"
      ],
